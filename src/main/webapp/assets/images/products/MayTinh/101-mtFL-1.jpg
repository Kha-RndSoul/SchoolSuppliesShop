	 else {
		 if(TL.Cart.notiMin != '') {
			 $('#modalNotiCart .modal-body').html(TL.Cart.notiMin);
			 $('#modalNotiCart').modal('show');
			 $('.btn-proceed-checkout-mobile').removeClass('loading');
		 }
