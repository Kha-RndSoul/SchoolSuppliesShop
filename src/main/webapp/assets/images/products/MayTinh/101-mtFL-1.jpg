	<script>
const createBatchedFunc = (func) => {
  let running = false;
  let callbacks = [];
  return (callback) => {
    callbacks.push(callback);
    if (running) return;
    running = true;
    func((...args) => {
      const tmp_callbacks = [...callbacks];
      running = false;
      callbacks = []; 
      for (const callback of tmp_callbacks) {
        callback(...args);
      }
    })
  }
};

const PE = {
	cart: null,
	getCart: createBatchedFunc(function(callback){
